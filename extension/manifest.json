{
	"name": "Bias Classifier",
	"description": "Classifies Bias (hehe)",
	"version": "1.0",
	"author": "@tassossapalidis",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs", "activeTab", "storage", "activeTab", "scripting"],
	"action": {
		"default_popup": "popup.html",
