			"16": "/logo.png",
			"32": "/logo.png",
			"48": "/logo.png",
			"128": "/logo.png"
		}
	},
	"icons": {
		"16": "/logo.png",
		"32": "/logo.png",
		"48": "/logo.png",
		"128": "/logo.png"
	}
}